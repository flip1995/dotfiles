{
    "rust-analyzer.serverPath": "~/.cargo/bin/rust-analyzer",
    "languageserver": {
        "bash": {
            "command": "bash-language-server",
            "args": ["start"],
            "filetypes": ["sh"],
            "ignoredRootPaths": ["~"]
        }
    },
    "snippets.priority": 100,
    "suggest.noselect": false
}
