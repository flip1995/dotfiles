{
    "coc.preferences.formatOnSaveFiletypes": ["go"],
    "rust-analyzer.serverPath": "~/.rustup/toolchains/nightly-x86_64-unknown-linux-gnu/bin/rust-analyzer",
    "languageserver": {
        "bash": {
            "command": "bash-language-server",
            "args": ["start"],
            "filetypes": ["sh"],
            "ignoredRootPaths": ["~"]
        }
    },
    "go.checkForUpdates": "ask",
    "snippets.priority": 100
}
